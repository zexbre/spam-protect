{
    "name": "zexbre/spam-protect",
    "description": "A simple, driver based spam protection package.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/zexbre/spam-protect/issues",
        "source": "https://github.com/zexbre/spam-protect"
    },
    "authors": [
        {
            "name": "Nikola Zeravcic",
            "email": "niks986@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "suggest": {
        "zexbre/mathguard": "Needed for support MathGuard verificator."
    },
    "autoload": {
        "psr-4": {
            "ZexBre\\SpamProtect\\": "src/SpamProtect/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZexBre\\SpamProtect\\": "tests/unit/SpamProtect/"
        }
    }
}
